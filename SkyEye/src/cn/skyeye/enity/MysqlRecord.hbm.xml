<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.skyeye.enity.MysqlRecord" table="MysqlRecord">

		<id name="mRecordId" column="mRecordId" type="string">
			<generator class="uuid" />
		</id>
		<property name="connectNum"  type="integer"/>
		<property name="aliveThread"  type="integer"/>
		<property name="sendSpeed"  type="float"/>
		<property name="receiveSpeed"  type="float"/>	
		<property name="perSearch"  type="integer"/>	
		<property name="perTransaction"  type="integer"/>	
		<property name="time"  type="long"/>	
	</class>
	
</hibernate-mapping>
