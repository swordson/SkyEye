<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.skyeye.enity.AlarmRecord" table="AlarmRecord">
		<id name="aRid" column="aRid" type="string">
			<generator class="uuid" />
		</id>
		<property name="item" column="item" type="string" />
		<property name="reason" column="reason" type="string"/>
		<property name="time" column="time" type="long"/>
		
	</class>
</hibernate-mapping>
